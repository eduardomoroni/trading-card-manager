{
  "name": "app",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "android": "react-native run-android",
    "android:build": "bundle exec fastlane android build",
    "ci:check": "scripts/ci-check.sh",
    "clean": "scripts/clear-cache.sh",
    "eslint": "eslint src/**",
    "eslint:fix": "eslint --fix src/** && npm run prettier",
    "flow": "flow",
    "flow:coverage": "flow coverage",
    "flow:install-libdefs": "flow-typed install",
    "info": "node node_modules/react-native/local-cli/cli.js info",
    "ios": "react-native run-ios",
    "ios:build": "bundle exec fastlane ios build",
    "postinstall": "scripts/post-install.sh",
    "preandroid": "scripts/android-emulator.sh start",
    "precommit": "lint-staged",
    "preinstall": "chmod -R +x scripts && scripts/pre-install.sh",
    "prettier": "prettier --write 'src/**/*.js'",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest --verbose",
    "test:coverage": "jest --silent --coverage && codecov",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "lodash": "4.17.10",
    "prop-types": "15.6.1",
    "react": "16.3.1",
    "react-native": "0.56.0",
    "react-native-config": "0.11.5",
    "react-native-firebase": "4.2.0",
    "react-native-i18n": "2.0.12",
    "react-native-navigation": "2.0.2295",
    "react-native-vector-icons": "4.6.0",
    "react-redux": "5.0.7",
    "recompose": "0.27.1",
    "redux": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.0",
    "validator": "10.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-jest": "22.4.3",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react-native": "4.0.0",
    "codecov": "3.0.2",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-prettier": "2.6.0",
    "flow-bin": "0.67.1",
    "flow-typed": "2.4.0",
    "husky": "0.14.3",
    "jest": "22.4.3",
    "lint-staged": "7.1.3",
    "prettier": "1.13.4",
    "react-dom": "16.4.0",
    "react-test-renderer": "16.3.1",
    "redux-mock-store": "1.5.1"
  },
  "jest": {
    "preset": "react-native",
    "coverageDirectory": "<rootDir>/coverage/",
    "setupTestFrameworkScriptFile": "<rootDir>/src/__tests__/setup.js",
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)"
    ]
  },
  "engines": {
    "node": "10.1.0",
    "npm": "6.0.1"
  },
  "lint-staged": {
    "*.js": [
      "npm run eslint:fix --",
      "git add"
    ]
  }
}
