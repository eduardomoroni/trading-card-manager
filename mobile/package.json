{
  "name": "app",
  "version": "0.0.1",
  "private": false,
  "engines": {
    "node": "10.13.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "android": "react-native run-android",
    "android:build": "bundle exec fastlane android build",
    "android:dev-menu": "adb shell input keyevent 82",
    "ci:check": "scripts/ci-check.sh",
    "clean": "scripts/clean.sh",
    "eslint": "eslint src/**",
    "eslint:fix": "eslint --fix src/** && npm run prettier",
    "flow": "flow",
    "flow:coverage": "flow coverage",
    "flow:install-libdefs": "flow-typed install",
    "info": "node node_modules/react-native/local-cli/cli.js info",
    "ios": "react-native run-ios",
    "ios:build": "bundle exec fastlane ios build",
    "postinstall": "scripts/post-install.sh",
    "preandroid": "scripts/android-emulator.sh start",
    "precommit": "lint-staged",
    "preinstall": "chmod -R +x scripts && scripts/pre-install.sh",
    "prettier": "prettier --write 'src/**/*.js'",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest --verbose",
    "test:coverage": "jest --silent --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "core": "file:../core",
    "data": "file:src/data",
    "lodash": "4.17.11",
    "moment": "2.22.2",
    "presentation": "file:src/presentation",
    "prop-types": "15.6.2",
    "react": "16.6.3",
    "react-native": "0.57.7",
    "react-native-action-button": "2.8.5",
    "react-native-config": "0.11.7",
    "react-native-firebase": "5.1.1",
    "react-native-i18n": "2.0.15",
    "react-native-navigation": "2.2.1",
    "react-native-vector-icons": "6.1.0",
    "react-redux": "6.0.0",
    "recompose": "0.30.0",
    "redux": "4.0.1",
    "validator": "10.9.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react-native": "4.0.1",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "5.10.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-prettier": "3.0.0",
    "flow-bin": "0.88.0",
    "flow-typed": "2.5.1",
    "jest": "23.6.0",
    "jest-enzyme": "7.0.1",
    "jest-runner-eslint": "0.7.1",
    "prettier": "1.15.3",
    "prettierrc": "0.0.0",
    "react-dom": "16.6.3",
    "react-native-cli": "2.0.1",
    "react-test-renderer": "16.6.3",
    "redux-mock-store": "1.5.3"
  }
}
