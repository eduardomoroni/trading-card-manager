#!/usr/bin/env bash

  if [ -f .env ]; then
      echo "Removing .env"
      rm .env
  fi

touch .env
cat <<EOT >> .env
FIREBASE_API_KEY=${FIREBASE_API_KEY}
FIREBASE_AUTH_DOMAIN=${FIREBASE_AUTH_DOMAIN}
FIREBASE_DATABASE_URL=${FIREBASE_DATABASE_URL}
FIREBASE_STORAGE_BUCKET=${FIREBASE_STORAGE_BUCKET}
FIREBASE_MSG_SENDER_ID=${FIREBASE_MSG_SENDER_ID}
FIREBASE_PROJECT_ID=${FIREBASE_PROJECT_ID}
LOG_ROCKET_PROJECT_ID=${LOG_ROCKET_PROJECT_ID}
BUGSNAG_API_KEY=${BUGSNAG_API_KEY}
REST_API_BASE_URL=${REST_API_BASE_URL}
GRAPHQL_API_URL=${GRAPHQL_API_URL}
GOOGLE_IOS_CLIENT_ID=${GOOGLE_IOS_CLIENT_ID}
GOOGLE_ANDROID_CLIENT_ID=${GOOGLE_ANDROID_CLIENT_ID}
FACEBOOK_APP_ID=${FACEBOOK_APP_ID}
EOT
