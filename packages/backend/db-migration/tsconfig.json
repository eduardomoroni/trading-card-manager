{
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "dist",
    "lib": ["es2016", "esnext.asynciterable"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
  }
}
