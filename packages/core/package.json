{
  "name": "core",
  "version": "0.0.1",
  "private": false,
  "main": "dist/index.js",
  "types": "index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "flow": "flow",
    "flow:coverage": "flow coverage",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --silent --coverage",
    "compile": "tsc --build tsconfig.json",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:watch": "cross-env NODE_ENV=development npm run build:src -- --watch --source-maps",
    "build:types": "tsc --declaration --outDir dist --emitDeclarationOnly",
    "build:js": "babel src --out-dir ./dist --extensions \".ts,.tsx\" --source-maps inline --delete-dir-on-start --copy-files",
    "build": "cross-env NODE_ENV=production npm run build:types && npm run build:js"
  },
  "dependencies": {
    "@types/node": "12.0.2",
    "moment": "2.22.2",
    "redux": "4.0.1",
    "redux-saga": "1.0.2",
    "validator": "11.0.0"
  }
}
