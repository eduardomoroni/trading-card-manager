{
  "groups": {
    "web": {
      "packages": [
        "core/package.json",
        "package.json",
        "web/package.json"
      ],
      "ignore": ["flow-bin"]
    },
    "mobile": {
      "packages": [
        "core/package.json",
        "mobile/package.json",
        "mobile/src/data/package.json",
        "mobile/src/presentation/package.json",
        "package.json"
      ],
      "ignore": ["react", "react-native", "react-native-navigation", "flow-bin"]
    },
    "backend": {
      "packages": [
        "backend/functions/package.json",
        "backend/package.json",
        "package.json"
      ]
    }
  }
}
