{
  "name": "core",
  "version": "0.0.1",
  "private": false,
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "npm run build",
    "flow": "flow",
    "flow:coverage": "flow coverage",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --silent --coverage",
    "build:src": "babel ./src --out-dir ./dist --delete-dir-on-start --copy-files",
    "build:watch": "NODE_ENV=development npm run build:src -- --watch --source-maps",
    "build": "NODE_ENV=production npm run build:src"
  },
  "dependencies": {
    "moment": "2.22.2",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.2",
    "validator": "10.9.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.0",
    "@babel/core": "7.2.0",
    "@babel/plugin-proposal-class-properties": "7.2.1",
    "@babel/preset-env": "7.2.0",
    "@babel/preset-flow": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "jest": "23.6.0",
    "redux-mock-store": "1.5.3"
  }
}
