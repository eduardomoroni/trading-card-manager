    "@babel/preset-env": "7.3.1",
    "@babel/preset-flow": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "cross-env": "5.2.0",
    "jest": "23.6.0",
